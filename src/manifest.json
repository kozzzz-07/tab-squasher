{
  "name": "Tab Squasher",
  "description": "ピン留されているやつ以外の重複したタブを削除するやつ",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "Tab Squasher",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs"]
}
